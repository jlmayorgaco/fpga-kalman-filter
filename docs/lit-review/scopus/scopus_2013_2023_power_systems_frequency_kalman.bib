	affiliations = {Guangxi Clean Energy Branch, China Huaneng Internatioal Electric Co., Ltd., Nanning City, China; Zhejiang Windey Co., Ltd., Key Laboratory of Wind Power Technology of Zhejiang Province, Hangzhou City, China; Guangxi Branch, China Huaneng Group Co., Ltd., Nanning City, China},
