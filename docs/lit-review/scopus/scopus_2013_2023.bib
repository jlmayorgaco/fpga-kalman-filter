	affiliations = {Department of Electrical and Computer Engineering, Florida International University, Miami, 33174, FL, United States; Qatar Mobility Innovations Center, Qatar University, Doha, 210531, Qatar; Department of Electrical and Computer Engineering, North Carolina State University, Raleigh, 27695, NC, United States},
