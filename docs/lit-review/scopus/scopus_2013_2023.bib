	year = {2023},
	journal = {IEEE Robotics and Automation Letters},
	volume = {8},
	number = {12},
	pages = {8565 – 8572},
	doi = {10.1109/LRA.2023.3330662},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85177055205&doi=10.1109%2fLRA.2023.3330662&partnerID=40&md5=7900aeab08b1483bd27ac6bbd1951545},
	affiliations = {Kth Royal Institute of Technology, Division of Robotics, Perception and Learning, Stockholm, 11428, Sweden; Digital Futures, Stockholm, 11428, Sweden; Delft University of Technology (TUD), Department of Cognitive Robotics (CoR), Delft, 2628CD, Netherlands},
	abstract = {Ensuring safety in real-world robotic systems is often challenging due to unmodeled disturbances and noisy sensors. To account for such stochastic uncertainties, many robotic systems leverage probabilistic state estimators such as Kalman filters to obtain a robot's belief, i.e. a probability distribution over possible states. We propose belief control barrier functions (BCBFs) to enable risk-aware control, leveraging all information provided by state estimators. This allows robots to stay in predefined safety regions with desired confidence under these stochastic uncertainties. BCBFs are general and can be applied to a variety of robots that use extended Kalman filters as state estimator. We demonstrate BCBFs on a quadrotor that is exposed to external disturbances and varying sensing conditions. Our results show improved safety compared to traditional state-based approaches while allowing control frequencies of up to 1 kHz.  © 2016 IEEE.},
	author_keywords = {Robot Safety; Sensor-based Control},
	keywords = {Kalman filters; Probability distributions; Random processes; Reactive power; Safety engineering; State estimation; Stochastic systems; Barriers functions; Control barriers; Quad rotors; Risk aware; Robot safety; Robot sensing system; Robotic systems; Sensor-based control; State Estimators; Uncertainty; Robots},
	correspondence_address = {M. Vahs; Kth Royal Institute of Technology, Division of Robotics, Perception and Learning, Stockholm, 11428, Sweden; email: vahs@kth.se},
